{
  "name": "@webx/sig",
  "version": "2.0.1",
  "description": "Quantum-Safe Cryptographically Secure ID Generator with Advanced Security Features",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "sig": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "test": "echo \"No tests specified\" && exit 0",
    "lint": "prettier --check \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "version:patch": "npm version patch && git push && git push --tags",
    "version:minor": "npm version minor && git push && git push --tags",
    "version:major": "npm version major && git push && git push --tags",
    "release:patch": "npm run version:patch",
    "release:minor": "npm run version:minor",
    "release:major": "npm run version:major"
  },
  "keywords": [
    "id-generator",
    "secure-id",
    "quantum-safe",
    "cryptography",
    "csprng",
    "hmac",
    "sha3",
    "blake2b",
    "uuid",
    "nanoid",
    "authentication",
    "distributed-systems"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/webx/sig.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "commander": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0"
  }
}
